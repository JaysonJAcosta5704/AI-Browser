{
  "manifest_version": 3,
  "name": "AI Browser",
  "version": "1.0",
  "description": "Your AI Accessor Extension",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_icon": "images/icon.png",
    "default_popup": "html/welcome.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
} 
